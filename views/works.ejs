<%- include("partials/header.ejs") %>
    
<div class="container box-day">
    <h1>Work List</h1>
</div>

<div class="container">
    <form action="/work" method="POST">
        <ul id="view" ">
            <% for (let i = 0; i < newWorkList.length; i++) {%>
                <div class="checkbox item">
                    <input type="checkbox" name="packersOff" id="packers" value="1"/>
                    <label for="packers" class="strikethrough"><%= newWorkList[i] %></label>
                </div>              
            <% } %>
            <div class="input-box">
                <input type='text' name="text" id="inputText" autocomplete="off" placeholder="New work" class="input-placeholder"/>
            <button class="add-btn">add</button> 
            </div>
            
        </ul>   
    </form>
</div>

    <%- include("partials/footer.ejs") %>

